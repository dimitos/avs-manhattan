<div class="pagination unselectable">
    <button class="btn btn__pagination btn__pagination_prev" type="button">
        {include "./icon.tpl" icon=["class" => "btn__pagination-icon icon__arrow-left", "id" => "arrow-left"]}
    </button>
    <div class="btn pagination-item active">1</div>
    <div class="btn pagination-item">2</div>
    <div class="btn pagination-item">3</div>
    <div class="btn pagination-item">4</div>
    <button class="btn btn__pagination btn__pagination_next active" type="button">
        {include "./icon.tpl" icon=["class" => "btn__pagination-icon icon__arrow-right", "id" => "arrow-right"]}
    </button>
</div>
